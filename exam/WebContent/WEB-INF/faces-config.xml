<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>com.controller.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>addQuesBean</managed-bean-name>
		<managed-bean-class>com.controller.AddQuestionBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>examPaperBean</managed-bean-name>
		<managed-bean-class>com.controller.ExamPaperBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>examPaperListBean</managed-bean-name>
		<managed-bean-class>com.controller.ExamPaperListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>examBean</managed-bean-name>
		<managed-bean-class>com.controller.ExamBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>com.controller.RegisterBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>examInfoBean</managed-bean-name>
		<managed-bean-class>com.controller.ExamPaperInfoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>examResultBean</managed-bean-name>
		<managed-bean-class>com.controller.ExamResultBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageStudentBean</managed-bean-name>
		<managed-bean-class>com.controller.ManageStudentBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>student</from-outcome>
			<to-view-id>/examPaperList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>userRegistration</from-outcome>
			<to-view-id>/registration.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/registration.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/addQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Save</from-outcome>
			<to-view-id>/addQuestion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>uploadQuestion</from-outcome>
			<to-view-id>/addQuestion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>manageQuestion</from-outcome>
			<to-view-id>/addQuestion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>setPaper</from-outcome>
			<to-view-id>/setExamPaper.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>examPaperList</from-outcome>
			<to-view-id>/examPaperList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>startExam</from-outcome>
			<to-view-id>/examPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/examPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>examResultPage</from-outcome>
			<to-view-id>/examResult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/examInformation.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>examPage</from-outcome>
			<to-view-id>/examPage.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>examListPage</from-outcome>
			<to-view-id>/examPaperList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/examPaperList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>examInfoPage</from-outcome>
			<to-view-id>/examInformation.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/examResult.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/manage_student.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>statchange</from-outcome>
			<to-view-id>/manage_student.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
